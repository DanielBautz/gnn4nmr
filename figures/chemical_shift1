        <!-- Bedeutung für die Strukturaufklärung -->
        <mxCell id="importance_title" value="&lt;font style=&quot;font-size: 18px;&quot; color=&quot;#004e9f&quot;&gt;&lt;b&gt;Bedeutung für die Strukturaufklärung&lt;/b&gt;&lt;/font&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" parent="1" vertex="1">
          <mxGeometry x="40" y="760" width="1020" height="30" as="geometry" />
        </mxCell>
        
        <mxCell id="importance_text" value="&lt;div style=&quot;text-align: center;&quot;&gt;&lt;font style=&quot;font-size: 14px;&quot;&gt;Die chemische Verschiebung ist ein entscheidendes Merkmal in der NMR-Spektroskopie, das Informationen über die chemische Umgebung von Protonen liefert. Sie ermöglicht die Identifizierung funktioneller Gruppen und die Aufklärung molekularer Strukturen. Zusammen mit anderen NMR-Parametern wie Kopplungskonstanten, Signalintensitäten und Multiplizitäten bildet sie die Grundlage für die moderne Strukturaufklärung in der organischen Chemie.&lt;/font&gt;&lt;/div&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;" parent="1" vertex="1">
          <mxGeometry x="60" y="790" width="980" height="60" as="geometry" />
        </mxCell>
